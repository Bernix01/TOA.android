<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="toa.toa">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="@string/title_activity_register"
            android:parentActivityName=".Splash_Activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.Splash_Activity" />
        </activity>
        <activity
            android:name=".Splash_Activity"
            android:label="@string/title_activity_splash_" >
        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name=".FirstVisit"
            android:label="@string/title_activity_first_visit" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_first_visit"
            android:parentActivityName=".Splash_Activity" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="toa.toa.Splash_Activity" />

        <activity
            android:name=".FirstTime"
            android:label="@string/title_activity_first_time"
            android:parentActivityName=".Splash_Activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.Splash_Activity" />
        </activity>
        <activity
            android:name=".LoadingSplash"
            android:label="@string/title_activity_loading_splash" />
        <activity
            android:name=".EditProfileActivity"
            android:label="@string/title_activity_edit_profile"
            android:parentActivityName=".ProfileActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.ProfileActivity" />
        </activity>
        <activity
            android:name=".activities.DetailconsejosNutricionalesActivity"
            android:label="@string/title_activity_consejos_nutricionales"
            android:parentActivityName=".activities.ConsejosNutricionalesActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.activities.ConsejosNutricionalesActivity" />
        </activity>
        <activity
            android:name=".ProfileActivity"
            android:label="@string/title_activity_profile"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.MainActivity" />
        </activity>
        <activity
            android:name=".FriendsActivity"
            android:label="@string/title_activity_friends"
            android:parentActivityName=".ProfileActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.ProfileActivity" />
        </activity>
        <activity
            android:name=".activities.RunningActivity"
            android:label="@string/title_activity_running">
        </activity>
        <activity
            android:name=".activities.CyclingActivity"
            android:label="@string/title_activity_cycling" />
        <activity
            android:name=".activities.FootballActivity"
            android:label="@string/title_activity_footbal" />
        <activity
            android:name=".activities.SwimmingActivity"
            android:label="@string/title_activity_swimming" />
        <activity
            android:name=".activities.TriathlonActivity"
            android:label="@string/title_activity_triathlon" />
        <activity
            android:name=".activities.ComunityActivity"
            android:label="@string/title_activity_atletismo_sport"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.MainActivity" />
        </activity>
        <activity
            android:name=".activities.ConsejosNutricionalesActivity"
            android:label="@string/title_activity_consejos_nutricionales" />
        <activity
            android:name=".AgendaActivity"
            android:label="@string/title_activity_agenda"
            android:parentActivityName=".ProfileActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.ProfileActivity" />
        </activity>
        <activity
            android:name=".AddfriendActivty"
            android:label="@string/title_activity_addfriend_activty"
            android:parentActivityName=".FriendsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.FriendsActivity" />
        </activity>
    </application>

</manifest>

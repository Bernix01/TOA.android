<?xml version="1.0" encoding="utf-8"?><!--   ~ Copyright TOA Inc. 2015. -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="toa.toa">

    <permission
        android:name="toa.toa.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <permission
        android:name="toa.toa.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="toa.toa.permission.C2D_MESSAGE" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="@string/title_activity_register"
            android:launchMode="singleTask" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:name=".FirstVisit"
            android:label="@string/title_activity_first_visit" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_first_visit"
            android:launchMode="singleTop"
            android:parentActivityName=".RegisterActivity" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="toa.toa.RegisterActivity" />

        <activity
            android:name=".FirstTime"
            android:label="@string/title_activity_first_time" />
        <activity
            android:name=".LoadingSplash"
            android:label="@string/title_activity_loading_splash" />
        <activity
            android:name=".EditProfileActivity"
            android:label="@string/title_activity_edit_profile"
            android:parentActivityName=".ProfileActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.ProfileActivity" />
        </activity>
        <activity
            android:name=".activities.DetailconsejosNutricionalesActivity"
            android:label="@string/title_activity_consejos_nutricionales"
            android:parentActivityName=".activities.ConsejosNutricionalesActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.activities.ConsejosNutricionalesActivity" />
        </activity>
        <activity
            android:name=".ProfileActivity"
            android:label="@string/title_activity_profile"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.MainActivity" />
        </activity>
        <activity
            android:name=".FriendsActivity"
            android:label="@string/title_activity_friends"
            android:parentActivityName=".ProfileActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.ProfileActivity" />
        </activity>
        <activity
            android:name=".activities.ComunityActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.MainActivity" />
        </activity>
        <activity
            android:name=".activities.ConsejosNutricionalesActivity"
            android:label="@string/title_activity_consejos_nutricionales" />
        <activity
            android:name=".AgendaActivity"
            android:label="@string/title_activity_agenda"
            android:parentActivityName=".ProfileActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.ProfileActivity" />
        </activity>
        <activity
            android:name=".AddfriendActivty"
            android:label="@string/title_activity_addfriend_activty"
            android:launchMode="singleTask"
            android:parentActivityName=".FriendsActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="toa.toa.FriendsActivity" />
        </activity>
        <activity
            android:name=".map.MapActivity"
            android:label="@string/app_name">
        </activity>

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyDmAHPu_iQcXk-KwhXB_BMgBKUSDCMRgtA" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <receiver
            android:name="com.microsoft.windowsazure.notifications.NotificationsBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />

                <category android:name="toa.toa" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".utils.RecoverPWActivity"
            android:label="@string/title_activity_recover_pw"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LoginActivity" />
        </activity>
    </application>

</manifest>
